{
    "name": "@es-joy/escodegen",
    "version": "4.0.0",
    "description": "ECMAScript code generator",
    "homepage": "http://github.com/es-joy/escodegen",
    "bugs": "http://github.com/es-joy/escodegen/issues",
    "type": "module",
    "main": "./dist/escodegen.cjs",
    "exports": {
        "node": {
            "types": "./dist/escodegen-node.d.ts",
            "import": "./dist/escodegen-node.esm.min.js",
            "require": "./dist/escodegen.cjs"
        },
        "browser-import": "./dist/escodegen-browser.esm.min.js",
        "browser": "./dist/escodegen-browser.umd.min.js"
    },
    "bin": {
        "esgenerate": "./bin/esgenerate.js",
        "escodegen": "./bin/escodegen.js"
    },
    "keywords": [
        "code",
        "generation"
    ],
    "files": [
        "LICENSE.BSD",
        "README.md",
        "bin",
        "dist",
        "package.json"
    ],
    "engines": {
        "node": ">=6.0"
    },
    "author": {
        "name": "Yusuke Suzuki",
        "email": "utatane.tea@gmail.com",
        "web": "http://github.com/Constellation"
    },
    "contributors": [],
    "repository": {
        "type": "git",
        "url": "http://github.com/es-joy/escodegen.git"
    },
    "dependencies": {
        "@es-joy/estraverse": "^7.1.1",
        "esprima": "^4.0.1",
        "esutils": "^2.0.3",
        "optionator": "^0.9.4"
    },
    "optionalDependencies": {
        "source-map": "~0.7.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-replace": "^6.0.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/esutils": "^2.0.2",
        "acorn": "^8.15.0",
        "babel-plugin-transform-es2017-object-entries": "0.0.5",
        "c8": "^10.1.3",
        "chai": "^6.0.1",
        "chai-exclude": "^3.0.1",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "mocha": "^11.7.2",
        "rollup": "^4.52.2",
        "source-map": "~0.7.6",
        "typescript": "^5.9.2"
    },
    "license": "BSD-2-Clause",
    "c8": {
        "checkCoverage": true,
        "branches": 100,
        "lines": 100,
        "functions": 100,
        "statements": 100,
        "reporter": [
            "lcov",
            "text"
        ],
        "exclude": [
            "test"
        ]
    },
    "scripts": {
        "tsc": "tsc",
        "example-escodegen": "./bin/escodegen.js benchmark/old.cjs",
        "example-generate": "./bin/esgenerate.js benchmark/asts/backbone-0.5.3-ast.json",
        "build": "rollup -c",
        "lint": "eslint .",
        "mocha": "mocha --require chai/register-expect.js test/*.js --timeout 100000",
        "unit-test": "c8 npm run mocha",
        "test": "npm run build && npm run unit-test && npm run lint",
        "release": "node tools/release.js"
    }
}
